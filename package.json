{
  "name": "react-rxd",
  "description": "Realstax web frontend component library",
  "repository": {
    "type": "git",
    "url": "git@github.com:Realstax/react-rxd.git"
  },
  "scripts": {
    "build": "rm -rf dist && webpack",
    "lint": "eslint --fix --config .eslintrc src",
    "lint-ci": "eslint -c .eslintrc src -f junit -o $CIRCLE_TEST_REPORTS/eslint/report.xml",
    "precommit": "npm run build && npm run lint && npm test",
    "test": "karma start --single-run",
    "watch-test": "karma start",
    "compile": "babel --presets es2015,react,stage-0 -d lib/ src/ && cpy \"**/*.scss\" ../lib --parents --cwd=src",
    "prepublish": "npm run build"
  },
  "main": "dist/",
  "license": "MIT",
  "version": "1.2.2",
  "dependencies": {
    "bourbon": "^4.2.7",
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "node-bourbon": "^4.2.8",
    "react": "^15.4.0",
    "react-foundation-components": "^0.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.0.0",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "husky": "^0.11.4",
    "inline-css-webpack-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-cli": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.9.3",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "sass-loader": "^3.2.3",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-externals-plugin": "^1.0.0"
  }
}
